{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.test1hotelwebsite.online/api"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://api.test1hotelwebsite.online/api",
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "app/api/[...route]/route.ts": {
      "maxDuration": 30
    }
  }
}